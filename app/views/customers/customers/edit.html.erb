<h2>登録情報編集</h2>

<!-- エラーメッセージ表示-->
<% if @customer.errors.any? %>
	<h3><%= @customer.errors.count %>error occurrence</h3><!--エラーメッセ-->
	<ul>
		<% @customer.errors.full_messages.each do |message| %>
		<li><%= message %></li>
		<% end %>
	</ul>
<% end %>
<!-- エラーメッセージ表示-->

<%= form_for(@customer) do |f| %>

  <%= f.label :氏名%>
  <%= f.text_field :last_name %>
  <%= f.text_field :first_name %></br>

  <%= f.label :フリガナ %>
  <%= f.text_field :last_name_kana %>
  <%= f.text_field :first_name_kana %></br>

  <%= f.label :郵便番号 %>
  <%= f.text_field :postal_number %></br>

  <%= f.label :住所 %>
  <%= f.text_field :address %></br>

  <%= f.label :電話番号 %>
  <%= f.text_field :phone_number %></br>

  <%= f.label :メールアドレス %>
  <%= f.text_field :email %></br>

  <%= f.submit "変更を保存する", class: "btn btn-primary" %>

  <%= link_to "退会する", exit_path, class: "btn btn-danger" %>
<% end %>